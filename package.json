    "app-root-path": "3.0.0",
    "eslint": "7.2.0",
    "eslint-plugin-unused-imports": "0.1.3",
    "mocha": "7.2.0",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2",
    "typescript": "^3.9.5"
  }
}
